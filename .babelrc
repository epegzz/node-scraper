{
  "plugins": [
    "@babel/plugin-syntax-async-generators",
    "@babel/plugin-proposal-async-generator-functions",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "4"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "env": {
    "production": {
      "ignore": [
        "**/*.test.js",
        "__mocks__"
      ]
    },
    "testing": {
      "ignore": [
        "dist/**/*"
      ]
    }
  }
}