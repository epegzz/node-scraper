{
  "name": "@epegzz/node-scraper",
  "version": "1.0.0",
  "author": "Daniel Sch√§fer <epegzz@gmail.com>",
  "description": "A scraping lib based on superagent",
  "keywords": [
    "node",
    "scraping",
    "scraper"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epegzz/node-scraper"
  },
  "engineStrict": true,
  "main": "src/node-scraper.js",
  "scripts": {
    "test": "NODE_ENV=testing jest --verbose --forceExit",
    "precommit": "lint-staged",
    "watch-test": "NODE_ENV=testing jest --watch",
    "coverage": "NODE_ENV=testing jest && codecov",
    "build": "NODE_ENV=production babel src -d dist",
    "lint": "eslint src",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "codecov": "^3.0.1",
    "eslint": "^4.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1"
  }
}
