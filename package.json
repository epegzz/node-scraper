{
  "name": "@epegzz/superscraper",
  "version": "1.0.0",
  "author": "Daniel Sch√§fer <epegzz@gmail.com>",
  "description": "A scraping lib based on superagent",
  "keywords": [
    "scraping",
    "scraper"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epegzz/superscraper"
  },
  "engineStrict": true,
  "main": "dist/superscraper.js",
  "scripts": {
    "test": "NODE_ENV=testing jest --verbose --forceExit",
