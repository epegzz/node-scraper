    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1"
